.PHONY: all clean

all: 
	-$(MAKE) -C hashtable-int
	-$(MAKE) -C hashtable-int-micro16
	-$(MAKE) -C hashtable-str
	-$(MAKE) -C hashtable-str-micro16

clean: 
	-$(MAKE) -C hashtable-int clean
	-$(MAKE) -C hashtable-int-micro16 clean
	-$(MAKE) -C hashtable-str clean
	-$(MAKE) -C hashtable-str-micro16 clean

